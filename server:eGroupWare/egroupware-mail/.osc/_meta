<package name="egroupware-mail" project="server:eGroupWare">
  <title>Containerized mail-server for EGroupware</title>
  <description> This package installs Docker and docker-compose and use it to run Dovecot
 and Postfix in containers as mail-server for an EGroupware installation.
 The mail-server is administrated through EGroupware.
</description>
  <build>
    <disable/>
    <enable arch="x86_64" repository="Debian_10"/>
    <enable arch="x86_64" repository="xUbuntu_20.04"/>
    <enable arch="x86_64" repository="xUbuntu_18.04"/>
    <enable arch="x86_64" repository="xUbuntu_16.04"/>
    <enable arch="x86_64" repository="Debian_9.0"/>
    <enable arch="x86_64" repository="Debian_11"/>
    <enable arch="x86_64" repository="xUbuntu_22.04"/>
  </build>
  <publish>
    <enable/>
  </publish>
</package>

